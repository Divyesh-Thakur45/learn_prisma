generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // directUrl = env("DIRECT_URL")
}

model Notes {
  id    Int    @id @default(autoincrement())
  name  String @default(nanoid())
  price String @default(nanoid())
}
